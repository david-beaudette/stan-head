<launch>
  <node pkg="rqt_gui" 
        type="rqt_gui" 
        name="head_ui" 
  />
  <node pkg="actionizer" 
        type="joy_actionizer" 
        name="joy_actionizer" 
  />
  <node pkg="joy" 
        type="joy_node" 
        name="joystick_receiver">
    
    <param name="dev_ff" value="" />
    <param name="dev" value="/dev/input/js0" />
    <param name="deadzone" value="0.05" />
    <param name="coalesce_interval" value="0.05" />
    <param name="pub_diagnostics" value="False" />

  </node>
</launch>